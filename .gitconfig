[user]
  name = Timothy Sullivan
[push]
  default = nothing
[merge]
  keepBackup = false;
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
[core]
  excludesfile = ~/.gitignore_global
  ignorecase = false
  editor = vim -f
[alias]
  co = checkout
  cob = checkout -b
  lg = log --format='%Cred%h%Creset %s %Cgreen(%cr) %C(blue)<%an>%Creset%C(yellow)%d%Creset'
[hub]
  protocol = ssh
